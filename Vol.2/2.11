    {
        printf("Incorrect input");
        return 1;
    }
    else if ((x < 0) || (x > 10))
    {
        printf("Incorrect input data");
        return 2;
    }
    else
    {
        if (x == 0)
        {
            printf("Zero");
        }
        if (x == 1)
        {
            printf("Jeden");
        }
        if (x == 2)
        {
            printf("Dwa");
        }
        if (x == 3)
        {
            printf("Trzy");
        }
        if (x == 4)
        {
            printf("Cztery");
        }
        if (x == 5)
        {
            printf("Piec");
        }
        if (x == 6)
        {
            printf("Szesc");
        }
        if (x == 7)
        {
            printf("Siedem");
        }
        if (x == 8)
        {
            printf("Osiem");
        }
        if (x == 9)
        {
            printf("Dziewiec");
        }
        if (x == 10)
        {
            printf("Dziesiec");
        }
    }
    return 0;
}
