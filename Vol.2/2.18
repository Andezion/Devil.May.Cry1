#include <stdio.h>

int main()
{
    float x;
    int a;

    printf("Enter mark: ");
    a = scanf("%f", &x);
    if (a != 1)
    {
        printf("Incorrect input");
        return 1;
    }
    else
    {
        if (x < 0)
        {
            printf("Incorrect input");
            return 1;
        }
        if ((0 <= x) && (x <= 10))
        {
            printf("Ocena 2");
        }
        if ((11 <= x) && (x <= 13))
        {
            printf("Ocena 3");
        }
        if ((14 <= x) && (x <= 16))
        {
            printf("Ocena 4");
        }
        if ((17 <= x) && (x <= 20))
        {
            printf("Ocena 5");
        }
        if (x > 20)
        {
            printf("Incorrect input");
            return 1;
        }
    }
    return 0;
}
