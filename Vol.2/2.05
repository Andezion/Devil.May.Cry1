#include <stdio.h>

int main()
{
    int number;

    printf("Enter your number: ");
    int check = scanf("%i",&number);
    if(!check)
    {
        printf("Incorrect input");
        return 1;
    }

    int result = (number * number) / number;
    if(result)
    {
        printf("Not equal to zero");
    }
    else
    {
        printf("Equal to zero");
    }

    return 0;
}
